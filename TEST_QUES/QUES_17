def setZeroes(matrix):
    rows, cols = len(matrix), len(matrix[0])
    rows_to_zero = set()
    cols_to_zero = set()

    for r in range(rows):
        for c in range(cols):
            if matrix[r][c] == 0:
                rows_to_zero.add(r)
                cols_to_zero.add(c)

    for r in range(rows):
        for c in range(cols):
            if r in rows_to_zero or c in cols_to_zero:
                matrix[r][c] = 0

matrix = [[1, 1, 1], [1, 0, 1], [1, 1, 1]]
setZeroes(matrix)
print(matrix)  
