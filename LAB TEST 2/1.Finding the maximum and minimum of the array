def find_max_min(arr):
    if not arr:
        return None, None

    max_elem = arr[0]
    min_elem = arr[0]

    for elem in arr:
        if elem > max_elem:
            max_elem = elem
        if elem < min_elem:
            min_elem = elem

    return max_elem, min_elem

arr = [3, 1, 4, 1, 5, 9, 2, 6, 5, 3, 5]
print("Max and Min:", find_max_min(arr))
