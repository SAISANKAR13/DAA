def calculate_indices(nums1, nums2):
    count1 = sum(1 for num in nums1 if num in nums2)
    count2 = sum(1 for num in nums2 if num in nums1)
    return [count1, count2]

nums1 = [2, 3, 2]
nums2 = [1, 2]
print(calculate_indices(nums1, nums2))

nums1 = [4, 3, 2, 3, 1]
nums2 = [2, 2, 5, 2, 3, 6]
print(calculate_indices(nums1, nums2)) 
