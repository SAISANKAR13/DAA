def find_largest_number(nums):
    max_num = nums[0]
    for num in nums[1:]:
        if num > max_num:
            max_num = num
    return max_num

test_cases = [
    [1, 2, 3, 4, 5],
    [7, 7, 7, 7, 7],
    [-10, 2, 3, -4, 5]
]

for i, nums in enumerate(test_cases, start=1):
    result = find_largest_number(nums)
    print(f"Test Case {i}: Input: {nums}, Expected Output: {result}")
